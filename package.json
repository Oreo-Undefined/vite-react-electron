{
  "name": "vite-react-electron",
  "version": "1.0.0",
  "description": "A vite-react-electron + mobx application ",
  "main": "main.js",
  "scripts": {
    "start": "concurrently \"vite\" \"npm run electron\"",
    "electron": "mode=development electron-forge start",
    "build": "vite build && npm run make",
    "package": "sudo electron-forge package",
    "make": "sudo electron-forge make",
    "make:mac": "sudo electron-forge make --platform darwin",
    "publish": "electron-forge publish"
  },
  "homepage": ".",
  "repository": "https://github.com/huangshuai9411/vite-react-electron.git",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "antd": "^4.18.0",
    "mobx": "^6.3.10",
    "mobx-react-lite": "^3.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@vitejs/plugin-react": "^1.0.7",
    "concurrently": "^6.5.1",
    "electron": "^16.0.5",
    "less": "^4.1.2",
    "vite": "^2.7.2"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": ""
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "exercent"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
